            <specular r="0.2" g="0.2" b="0.2" a="1" />
        </omni>


        <spot id="ls1" enabled="true" angle="0.01" exponent="10">
            <location x="-7.2" y="3" z="10.2" w="1" />
            <target x="-7.3" y="2" z="10.2" />
            <ambient r="0" g="0" b="0" a="0" />
            <diffuse r="0.6" g="0.5" b="0.5" a="1" />
            <specular r="0.2" g="0.2" b="0.2" a="1" />
        </spot>
    </lights>
    <!--          -->
    <!-- TEXTURES -->
    <!--          -->
    <textures>
        <texture id="tt1" file="vidral.jpg" />
        <texture id="ttRoadLane" file="roadTwoLanes.png" />
